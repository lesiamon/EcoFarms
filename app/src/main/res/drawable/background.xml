<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <gradient
                android:startColor="#eef3f1"
                android:endColor="#72cc85"
                android:angle="150" />
        </shape>
    </item>
    <item
        android:left="0dp"
        android:right="0dp"
        android:top="2dp"
        android:bottom="2dp">
        <shape>
            <gradient
                android:startColor="#72cc85"
                android:endColor="#436659"
                android:angle="135" />
        </shape>
    </item>
</layer-list>
